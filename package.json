    "@dossierhq/server": "~0.7.21",
    "astro": "^5.15.4",
    "better-sqlite3": "~12.5.0",
    "leaflet": "~1.9.4",
    "react": "~18.3.1",
    "react-dom": "~18.3.1"
  },
  "devDependencies": {
